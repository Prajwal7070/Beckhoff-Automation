{
  "$schema": "http://json.schemastore.org/tsconfig",
  "compileOnSave": true,
  "compilerOptions": {
    "module": "none",
    "target": "ES2020",
    "skipLibCheck": true,
    "lib": [ "DOM", "DOM.Iterable", "ES2019", "ES2020.BigInt" ],
    "types": [],
    "declaration": true,
    "sourceMap": true,
    "noEmitOnError": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "strictBindCallApply": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "strictFunctionTypes": false,
    "strictPropertyInitialization": false,
    "alwaysStrict": false
  },
  "include": [
    ".hmiframework/TcHmi.d.ts",
    ".hmipkgs/Beckhoff.TwinCAT.HMI.Controls/TcHmiEllipse/TcHmiEllipse.d.ts"
  ],
  "files": [
    "test_frameworkControl/test_frameworkControl.ts"
  ]
}
